  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo \"No test specified\" && exit 0",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path .gitignore",
