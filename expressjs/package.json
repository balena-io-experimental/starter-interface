    "dev": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "balena-sdk": "^16.13.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
