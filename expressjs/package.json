  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo \"No test specified\" && exit 0",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path .gitignore",
    "formatcheck": "prettier \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path .gitignore --check",
    "dev": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "oddev": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "balena-sdk": "^16.19.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^5.0.0-beta.1",
    "express-slow-down": "^1.4.0",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "klaw-sync": "^6.0.0",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.21",
    "systeminformation": "^5.11.14",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-slow-down": "^1.3.2",
    "@types/formidable": "^2.0.4",
    "@types/fs-extra": "^9.0.13",
    "@types/klaw-sync": "^6.0.1",
    "@types/lockfile": "^1.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
