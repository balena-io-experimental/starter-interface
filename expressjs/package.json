  "repository": "https://github.com/balena-labs-research/starter-interface",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path ../.gitignore",
    "formatcheck": "prettier \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path ../.gitignore --check",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "balena-sdk": "^16.26.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^5.0.0-beta.1",
    "express-slow-down": "^1.5.0",
    "formidable": "^2.0.1",
    "fs-extra": "^10.1.0",
    "klaw-sync": "^6.0.0",
    "lockfile": "^1.0.4",
    "systeminformation": "^5.12.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-slow-down": "^1.3.2",
    "@types/formidable": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/klaw-sync": "^6.0.1",
    "@types/lockfile": "^1.0.2",
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
