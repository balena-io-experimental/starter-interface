  "author": "maggie0002",
  "repository": "https://github.com/maggie0002/balena-device-ui",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue,.json ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path .gitignore",
    "formatcheck": "prettier \"**/*.{js,ts,vue,scss,html,md}\" --ignore-path .gitignore --check",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "yarn lint -c .eslintrc-treeshake.js --no-eslintrc --fix && quasar build",
    "build-pwa": "yarn lint -c .eslintrc-treeshake.js --no-eslintrc --fix && quasar build -m pwa",
    "build-electron": "yarn lint -c .eslintrc-treeshake.js --no-eslintrc --fix && quasar build -m electron"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@quasar/extras": "^1.15.4",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "js-file-download": "^0.4.12",
    "pinia": "^2.0.22",
    "quasar": "^2.8.4",
    "vue": "^3.2.40",
    "vue-chart-3": "^3.1.8",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@quasar/app-vite": "^1.1.2",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "electron": "^21.0.1",
    "electron-packager": "^16.0.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
